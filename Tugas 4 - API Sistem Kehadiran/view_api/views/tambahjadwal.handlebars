<div class="container">
  <h4>Tambah Jadwal</h4>
  <hr>
  <form method="post" action="http://9f4292ba.ngrok.io/tambahjadwal">
      <div class="form-group">
          <label for="exampleInputEmail1">Matkul ID</label>
          <input name="idMatkul" type="text" class="form-control">
      </div>
      <div class="form-group">
          <label for="exampleInputEmail1">Pertemuan Ke</label>
          <input name="pertemuanKe" type="number" class="form-control">
      </div>
      <div class="form-group">
          <label for="exampleInputEmail1">Ruangan</label>
          <input name="ruang" type="text" class="form-control">
      </div>
      <div class="form-group">
          <label for="exampleInputEmail1">Jam Masuk</label>
          <input name="jamMasuk" type="datetime-local" class="form-control">
      </div>
      <div class="form-group">
          <label for="exampleInputEmail1">Jam Selesai</label>
          <input name="jamSelesai" type="datetime-local" class="form-control">
      </div>
      <div class="form-group">
        <label for="exampleInputEmail1">Tahun Ajaran</label>
        <input name="tahunAjaran" type="text" class="form-control">
      </div>
      <div class="form-group">
          <label for="exampleInputEmail1">Semester</label>
          <input name="semester" type="text" class="form-control">
      </div>
      <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>
